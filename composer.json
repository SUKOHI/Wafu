{
    "name": "sukohi/wafu",
    "description": "PHP class that mainly developed for Laravel to provide Japan-Related data like week name, gender and so on.",
    "authors": [
        {
            "name": "Sukohi",
            "email": "capilano.sukohi@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sukohi\\Wafu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Sukohi\\Wafu\\Tests\\": "packages/Wafu/tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Sukohi\\Wafu\\WafuServiceProvider"
            ],
            "aliases": {
                "Wafu": "Sukohi\\Wafu\\Facades\\Wafu"
            }
        }
    }
}
